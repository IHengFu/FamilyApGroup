<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:width="30dp"
        android:height="10dp">
        <shape>
            <solid android:color="#55ee55" />
        </shape>
    </item>
    <item
        android:width="30dp"
        android:height="10dp"
        android:left="30dp">
        <shape>
            <solid android:color="#ee5555" />
        </shape>
    </item>
    <item>
        <shape>
            <stroke
                android:width="1dp"
                android:color="@android:color/white" />
        </shape>
    </item>
</layer-list>
